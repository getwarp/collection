{
  "name": "spaceonfire/collection",
  "description": "Framework agnostic PHP collections",
  "type": "library",
  "keywords": [
    "collection"
  ],
  "homepage": "https://github.com/spaceonfire/collection",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://www.onfire.space",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "spaceonfire/common": "^3.0",
    "spaceonfire/criteria": "^3.0",
    "spaceonfire/type": "^3.0",
    "symfony/polyfill-php80": "^1.22"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "spaceonfire\\Collection\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spaceonfire\\Collection\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
