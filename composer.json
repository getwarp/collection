{
  "name": "getwarp/collection",
  "description": "Framework-agnostic PHP collections",
  "type": "library",
  "keywords": [
    "collection",
    "list",
    "map",
    "generators"
  ],
  "homepage": "https://github.com/getwarp/collection",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://getwarp.dev",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "getwarp/common": "^3.0",
    "getwarp/criteria": "^3.0",
    "getwarp/type": "^3.0",
    "symfony/polyfill-php80": "^1.22",
    "symfony/polyfill-php81": "^1.22"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Warp\\Collection\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Warp\\Collection\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
